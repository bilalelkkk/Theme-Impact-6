    <style>
  .status-box {
    display: flex;
    justify-content: space-around;
    align-items: flex-start;
    border: 2px solid #4f4f4f;
    border-radius: 0; /* Szgletes keret */
    padding: 20px;
    font-family: 'Arial', sans-serif;
    background: #fff;
    max-width: 700px;
    margin: auto;
    position: relative;
  }

  .status-step {
    text-align: center;
    flex: 1;
    position: relative;
    z-index: 2; /* Ikonrteg eltrbe */
  }

  .status-step:not(:last-child)::after {
    content: "";
    position: absolute;
    top: 25px;
    right: -50%;
    width: 100%;
    height: 2px;
    background: #ccc;
    z-index: 0; /* Htul legyen */
  }

  .status-icon {
    background: #f4f4f4;
    border-radius: 50%; /* Ikon marad kr alak */
    padding: 15px;
    width: 50px;
    height: 50px;
    margin: auto;
    margin-bottom: 10px;
    box-shadow: 0 0 10px rgba(0,0,0,0.1);
    display: flex;
    align-items: center;
    justify-content: center;
    z-index: 3; /* Ikon mg elrbb */
    position: relative; /* Szksges a z-index rvnyestshez */
  }

  .status-title {
    font-weight: bold;
    margin-bottom: 5px;
    font-size: 16px;
  }

  .status-date {
    font-size: 14px;
    color: #333;
    line-height: 1.3;
  }
</style>

<div class="status-box">
  <div class="status-step">
    <div class="status-icon"></div>
    <div class="status-title">Comandat</div>
    <div class="status-date" id="orderDate"></div>
  </div>
  <div class="status-step">
    <div class="status-icon"></div>
    <div class="status-title">Colet Trimis</div>
    <div class="status-date" id="shippedDate"></div>
  </div>
  <div class="status-step">
    <div class="status-icon"></div>
    <div class="status-title">Livrat</div>
    <div class="status-date" id="deliveredDate"></div>
  </div>
</div>

<script>
  const locale = 'ro-RO';

  const days = {
    'lun.': 'Lun',
    'mar.': 'Mar',
    'mie.': 'Mie',
    'joi.': 'Joi',
    'vin.': 'Vin',
    'sm.': 'Sm',
    'dum.': 'Dum'
  };

  const months = {
    'ian.': 'Ian',
    'feb.': 'Feb',
    'mar.': 'Mart',
    'apr.': 'Apr',
    'mai.': 'Mai',
    'iun.': 'Iun',
    'iul.': 'Iul',
    'aug.': 'Aug',
    'sept.': 'Sept',
    'oct.': 'Oct',
    'nov.': 'Nov',
    'dec.': 'Dec'
  };

  function formatDate(offset) {
    const d = new Date();
    d.setDate(d.getDate() + offset);
    let parts = d.toLocaleDateString(locale, { weekday: 'short', day: '2-digit', month: 'short' }).split(' ');

    const dayName = days[parts[0].toLowerCase()] || parts[0];
    const dayNumber = parts[1];
    const monthName = months[parts[2].toLowerCase()] || parts[2];

    return `${dayName} ${dayNumber} ${monthName}.`;
  }

  document.getElementById("orderDate").textContent = formatDate(0);
  document.getElementById("shippedDate").textContent = formatDate(2);
  document.getElementById("deliveredDate").textContent = formatDate(3);
</script>
